{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }

    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "import/extensions": "off",
        "no-underscore-dangle": "off",
        "no-plusplus": "off"
    }
}
